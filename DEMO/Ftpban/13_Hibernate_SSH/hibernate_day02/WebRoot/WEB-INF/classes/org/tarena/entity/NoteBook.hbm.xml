<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.tarena.entity.NoteBook" 
		table="cn_notebook">
		<id name="bookId" column="cn_notebook_id" 
			type="string">
		</id>
		<property name="userId" column="cn_user_id"
			type="string">
		</property>
		<property name="typeId" 
			column="cn_notebook_type_id" type="string">
		</property>
		<property name="name" column="cn_notebook_name"
			type="string">
		</property>
		<property name="descr" column="cn_notebook_desc"
			type="string">
		</property>
		<property name="creatime" 
			column="cn_notebook_createtime"
			type="timestamp">
		</property>
	</class>
</hibernate-mapping>